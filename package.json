    "@testing-library/user-event": "^13.5.0",
    "lexical": "^0.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
