{
  "name": "lexical-rte",
  "version": "0.1.0",
  "author": "arunpraba",
  "description": "It is a lexical based rich text editor",
  "keywords": [
    "react",
    "components",
    "lexical",
    "rte"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "private": false,
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@lexical/react": "^0.3.1",
    "@testing-library/jest-dom": "^5.16.4",
